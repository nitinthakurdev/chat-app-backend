{
    "version": 2,
    "framework": {
      "name": "vercel-node"
    },
    "builds": [
      {
        "src": "build/index.js",
        "use": "@vercel/bun",
        "config": {
        "runtime": "bun"
      }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/build/index.js"
      }
    ]
  }
  